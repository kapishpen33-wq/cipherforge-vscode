{
  "name": "promptmaster-pro",
  "displayName": "PromptMaster: AI Meta-Prompt",
  "description": "The AI Meta-Agent for high-context prompt engineering",
  "publisher": "kapishp",
  "version": "0.0.3",
  "engines": {
    "vscode": "^1.109.0"
  },
  "categories": ["Machine Learning", "Other"],
  "keywords": ["ai", "prompt", "gemini", "chatgpt", "copilot"],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "promptmaster-pro.optimizeCode",
        "title": "PromptMaster: Generate Master Prompt"
      },
      {
        "command": "promptmaster-pro.resetApiKey",
        "title": "PromptMaster: Reset API Key"
      }
    ],
    "menus": {
      "editor/context": [
        {
          "command": "promptmaster-pro.optimizeCode",
          "group": "navigation"
        }
      ]
    }
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "22.x",
    "@types/vscode": "^1.109.0",
    "@vscode/test-cli": "^0.0.12",
    "@vscode/test-electron": "^2.5.2",
    "eslint": "^9.39.2",
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "webpack": "^5.104.1",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@google/generative-ai": "^0.24.1"
  }
}
